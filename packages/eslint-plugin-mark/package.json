{
  "name": "eslint-plugin-mark",
  "version": "0.1.0-canary.0",
  "type": "module",
  "description": "Lint your Markdown with ESLint.üõ†Ô∏è",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./src/index.js"
    },
    "./rules": {
      "types": "./build/rules/index.d.ts",
      "default": "./src/rules/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./build/*"
      ],
      ".": [
        "./build/index.d.ts"
      ],
      "./rules": [
        "./build/rules/index.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "build",
    "LICENSE.md",
    "README.md",
    "!src/**/*.test.js",
    "!**/fixtures/**"
  ],
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "plugin",
    "markdown",
    "mark",
    "commonmark",
    "gfm"
  ],
  "author": "Î£®Î∞ÄLuMir <rpfos@naver.com> (https://github.com/lumirlumir)",
  "license": "MIT",
  "homepage": "https://eslint-plugin-mark.lumir.page",
  "repository": {
    "type": "github",
    "url": "git+https://github.com/lumirlumir/npm-eslint-plugin-mark.git",
    "directory": "packages/eslint-plugin-mark"
  },
  "bugs": {
    "url": "https://github.com/lumirlumir/npm-eslint-plugin-mark/issues"
  },
  "publishConfig": {
    "provenance": true
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npx tsc && shx cp ./src/core/types.d.ts ./build/core/types.d.ts && shx cp ../../LICENSE.md ../../README.md .",
    "test": "node --test"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "dependencies": {
    "@eslint/markdown": "^6.3.0",
    "@types/mdast": "^4.0.4"
  },
  "devDependencies": {
    "eslint": "^9.22.0"
  }
}
