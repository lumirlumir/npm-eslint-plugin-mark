{
  "name": "eslint-plugin-mark",
  "version": "0.1.0-canary.8",
  "type": "module",
  "description": "Lint your Markdown with ESLint.üõ†Ô∏è",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./src/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./build/*"
      ],
      ".": [
        "./build/index.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "build",
    "LICENSE.md",
    "README.md",
    "!src/**/*.test.{js,ts}",
    "!**/fixtures/**"
  ],
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "plugin",
    "markdown",
    "mark",
    "commonmark",
    "gfm"
  ],
  "author": "Î£®Î∞ÄLuMir <rpfos@naver.com> (https://github.com/lumirlumir)",
  "license": "MIT",
  "homepage": "https://eslint-plugin-mark.lumir.page",
  "repository": {
    "type": "github",
    "url": "git+https://github.com/lumirlumir/npm-eslint-plugin-mark.git",
    "directory": "packages/eslint-plugin-mark"
  },
  "bugs": {
    "url": "https://github.com/lumirlumir/npm-eslint-plugin-mark/issues"
  },
  "publishConfig": {
    "provenance": true
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npx tsc && node ../../scripts/cp.mjs ../../LICENSE.md LICENSE.md ../../README.md README.md",
    "test": "node --test"
  },
  "peerDependencies": {
    "eslint": "^9.15.0"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint/markdown": "^7.5.1",
    "emoji-regex": "^10.6.0",
    "micromark-util-normalize-identifier": "^2.0.1",
    "parse5": "^8.0.0"
  },
  "devDependencies": {
    "@types/mdast": "^4.0.4",
    "eslint": "^9.39.1"
  }
}
